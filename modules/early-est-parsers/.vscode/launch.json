  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/quakeml-file-importer/src/fileispects.js",
      "args": ["/Users/astaniscia/Workspace2/ingv/data/1571228544986_pilo1.xml"]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Mocha all tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": ["${workspaceRoot}/test/**/*.js"],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Current File",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": ["${file}"],
      "cwd": "${workspaceRoot}"
    }
  ]
}
